{
    "trailingComma": "es5",
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "importOrder": [
        "^@/lib/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^@/components/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "prettier-plugin-tailwindcss",
        "@trivago/prettier-plugin-sort-imports"
    ]
}
